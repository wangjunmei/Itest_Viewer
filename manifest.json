{
	"manifest_version": 2,

	"name": "Itest Viewer",
	"version": "2.1.0",
	"description": "View the location attributes supported by ITest",
	"author": "beyondsoft",

	"browser_action": {
		"default_icon": "icon.png"
	},

	"icons": {
		"16": "icon.png",
		"48": "icon48.png",
		"128": "icon128.png"
	},

	"background": {
		"scripts": ["event.js"]
	},

	"permissions": ["activeTab"],
	"web_accessible_resources": ["template.html"]
}
